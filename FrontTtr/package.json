{
  "name": "frontttr",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint src/**/*.{ts,tsx} --ignore-pattern src/app/**",
    "test": "jest --passWithNoTests",
    "preview": "vite preview",
    "prepare": "husky",
    "typecheck": "tsc -p tsconfig.app.json --noEmit && tsc -p tsconfig.node.json --noEmit"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint src/**/*.{ts,tsx} --fix --ignore-pattern src/app/**"
    ]
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "lint-staged": "^16.2.7",
    "ts-jest": "^29.4.6",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.52.0",
    "vite": "^7.1.2"
  }
}
